<%= form_tag(newspaper_boxes_path, method: :get) do %>
    Search by:
    Zip Code: <%= select_tag 'zip_code', options_for_select(NewspaperBox.zipcode_list, @selected_zip_code), prompt: 'All'%>
    City: <%= select_tag 'city', options_for_select(NewspaperBox.city_list, @selected_city), prompt: 'All' %>
    borough: <%= select_tag 'borough', options_for_select(NewspaperBox.borough_list, @selected_borough), prompt: 'All' %>
Trash Bin: <%= check_box_tag 'trash', true, @selected_trash %>
  </br>
  <%= button_tag 'submit'%>
<% end %>