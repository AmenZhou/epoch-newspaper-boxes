<h3>Newspaper Boxes List</h3>

<%= form_tag(newspaper_boxes_path, method: :get, class: 'form-inline') do %>
  <label>Select Table:</label>
  <%= select_tag 'table_type', options_for_select(["hand", "box"], "box"), class: 'form-control' %>
  <%= button_tag 'submit', class: 'btn btn-default'%>
<% end %>
<br>
<%= render "new_export_button" %>
<%= render "query_form" %>
<% if @selected_trash %>
  <%= render "trash_list"%>
<% else %>
  <%= render "box_list" %>
<% end %>

<br>

<script>
  $(document).ready(function(){
    $('.best_in_place').best_in_place();
  })
</script>
