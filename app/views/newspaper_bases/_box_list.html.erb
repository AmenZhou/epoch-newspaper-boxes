<%= paginate @newspaper_bases%>
<div class='box-list'>
  <table class="table table-border table-hover table-striped dataTable newspaper-table">
    <thead>
      <tr>
        <% model_name::ColumnName.each do |column| %>
         <th> <%= NewspaperBase::DisplayColumnName[column] %></th>
        <% end %>
      </tr>
    </thead>

    <tbody>
      <tr id="table_sum">
        <%= render "summary_row" %>
      </td>
      <% @newspaper_bases.each do |newspaper_base| %>
      <%#Todo change label name, make it more human readable %>
        <tr id="<%= newspaper_base.id%>">
          <% model_name::ColumnName.each do |column| %>
            <td><%= table_columns(newspaper_base)[column].call %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>